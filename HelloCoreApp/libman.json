{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/js/twitter-bootstrap/"
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/js/jquery/",
      "files": [
        "jquery.js",
        "jquery.min.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.1",
      "destination": "wwwroot/js/jquery-validate/",
      "files": [
        "jquery.validate.min.js",
        "jquery.validate.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/js/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.min.js",
        "jquery.validate.unobtrusive.js"
      ]
    }
  ]
}